{% trans_default_domain 'NovosgaReportsBundle' %}

{% for dado in relatorio.dados %}
    <div class="header">
        <h2>{{ dado.perfil }}</h2>
    </div>
    <table class="table table-bordered table-striped">
        <thead class="thead-light">
            <tr>
                <th>{% trans %}report.modules{% endtrans %}</th>
            </tr>
        </thead>
        <tbody>
            {% for permissao in dado.permissoes %}
                <tr>
                    <td>{{ permissao }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endfor %}