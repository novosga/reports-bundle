{
    "name": "novosga/reports-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": { "Novosga\\ReportsBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Novosga\\ReportsBundle\\Tests\\": "tests/" }
    },
    "require": {
        "php": ">=8.2",
        "doctrine/orm": "^3.2",
        "novosga/core": "2.2.x-dev",
        "symfony/framework-bundle": "7.1.*",
        "symfony/form": "7.1.*",
        "twig/twig": "^2.12|^3.0",
        "symfony/translation": "7.1.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "~8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
